{
  "name": "eoapi-tapdata",
  "version": "1.0.3",
  "description": "Import the JSON format file of the OpenAPI specification",
  "main": "dist/index.js",
  "title": "Import Tapdata",
  "author": "Tapdata",
  "logo": "https://data.eolink.com/4waW1cweaf7bfd75ed8d3c37a3c77628cfadd6bb60bb2b2",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js"
  },
  "devDependencies": {
    "esbuild": "^0.14.38",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1"
  },
  "features": {
    "importAPI": {
      "action": "importFunc",
      "label": "OpenAPI(.json)",
      "description": "${function.description}",
      "icon": "https://data.eolink.com/4waW1cweaf7bfd75ed8d3c37a3c77628cfadd6bb60bb2b2"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
