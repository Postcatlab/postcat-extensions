{
  "name": "postcat-export-openapi",
  "version": "1.0.4",
  "description": "Export the JSON format file of the OpenAPI specification",
  "main": "dist/index.js",
  "title": "Export OpenAPI",
  "logo": "https://data.eolink.com/1eBQg8B47dfbbfcfd73d542fae45a95309a073dfcdec3da",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eolinker/postcat-extensions.git"
  },
  "keywords": [
    "feature",
    "postcat"
  ],
  "author": "Postcat",
  "license": "Apache-2.0 License",
  "bugs": {
    "url": "https://github.com/eolinker/postcat-extensions/issues"
  },
  "homepage": "https://github.com/eolinker/postcat-extensions#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@types/lodash-es": "*",
    "@types/node": "^17.0.29",
    "esbuild": "^0.14.38",
    "nodemon": "^2.0.15",
    "rollup": "^2.78.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.3"
  },
  "categories": ["Data Migration"],
  "features": {
    "exportAPI": {
      "action": "export_convert",
      "label": "OpenAPI(.json)",
      "description": "${function.description}",
      "icon": "https://data.eolink.com/1eBQg8B47dfbbfcfd73d542fae45a95309a073dfcdec3da",
      "filename": "openapi.json"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  },
  "dependencies": {
    "lodash": "*",
    "lodash-es": "^4.17.21"
  }
}
