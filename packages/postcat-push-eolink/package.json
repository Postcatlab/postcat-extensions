{
  "name": "postcat-push-eolink",
  "version": "0.0.1",
  "description": "Export OpenAPI data to Eolink",
  "main": "dist/index.js",
  "title": "Push to Eolink",
  "logo": "https://data.eolink.com/sMhVC6D622a2f1c2bc2147d54d45b24ad14bcc0c900d406",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js",
    "test": "nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eolinker/postcat-extensions.git"
  },
  "keywords": [
    "feature",
    "postcat"
  ],
  "author": "Eolink",
  "license": "Apache-2.0 License",
  "bugs": {
    "url": "https://github.com/eolinker/postcat-extensions/issues"
  },
  "homepage": "https://eolink.com",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@types/lodash-es": "*",
    "esbuild": "^0.14.38",
    "nodemon": "^2.0.15",
    "rollup": "^2.78.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6"
  },
  "dependencies": {
    "lodash": "*",
    "lodash-es": "^4.17.21",
    "openapi-types": "^12.0.2"
  },
  "features": {
    "syncAPI": {
      "action": "sync_to_remote",
      "label": "Eolink",
      "description": "${function.description}",
      "icon": "https://data.eolink.com/sMhVC6D622a2f1c2bc2147d54d45b24ad14bcc0c900d406",
      "extestion": ""
    },
    "configuration": {
      "type": "object",
      "properties": {
        "eolink.token": {
          "type": "string",
          "required": false,
          "default": "",
          "label": "Token",
          "description": "${configuration.token.description}"
        },
        "eolink.projectID": {
          "type": "string",
          "required": false,
          "default": "",
          "label": "ProjectID",
          "description": "${configuration.projectId.description}"
        }
      }
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
