{
  "name": "postcat-import-openapi",
  "version": "1.0.8",
  "description": "Import the JSON format file of the OpenAPI/Swagger specification",
  "main": "dist/index.js",
  "title": "Import OpenAPI/Swagger",
  "author": "Postcat",
  "logo": "https://data.eolink.com/bxBDitte0fdc7007f09f0f85e91b48792c52b7affad5b1d",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Postcatlab/postcat-extensions.git"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "esbuild": "^0.14.38",
    "rollup": "^2.78.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1"
  },
  "categories": [
    "Data Migration"
  ],
  "features": {
    "importAPI": {
      "action": "importFunc",
      "type": "file",
      "label": "OpenAPI(.json)",
      "description": "${function.description}",
      "icon": "https://data.eolink.com/bxBDitte0fdc7007f09f0f85e91b48792c52b7affad5b1d"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
