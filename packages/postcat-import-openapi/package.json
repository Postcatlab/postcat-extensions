{
  "name": "postcat-import-openapi",
  "version": "1.0.2",
  "description": "Import the JSON format file of the OpenAPI/Swagger specification",
  "main": "dist/index.js",
  "title": "Import OpenAPI/Swagger",
  "author": "Postcat",
  "logo": "https://data.eolink.com/1eBQg8B47dfbbfcfd73d542fae45a95309a073dfcdec3da",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "esbuild": "^0.14.38",
    "rollup": "^2.78.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1"
  },
  "features": {
    "importAPI": {
      "action": "importFunc",
      "label": "OpenAPI(.json)",
      "description": "${function.description}",
      "icon": "https://data.eolink.com/1eBQg8B47dfbbfcfd73d542fae45a95309a073dfcdec3da"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
