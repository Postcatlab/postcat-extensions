{
  "name": "postcat-sync-swagger",
  "version": "0.0.1",
  "description": "Export the JSON format file of the Postcat specification",
  "main": "dist/index.js",
  "title": "Export Postcat",
  "logo": "https://data.eolink.com/hYftLZXaa924b3e221fec240ac7ffcad1bd0bbce6799c15",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eolinker/postcat-extensions.git"
  },
  "author": "Postcat",
  "license": "Apache-2.0 License",
  "bugs": {
    "url": "https://github.com/eolinker/postcat-extensions/issues"
  },
  "homepage": "https://github.com/eolinker/postcat-extensions#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@types/lodash-es": "*",
    "@types/node": "^17.0.29",
    "esbuild": "^0.14.38",
    "rollup": "^2.78.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.3"
  },
  "categories": ["Data Migration"],
  "features": {
    "updateAPI": {
      "action": "updateAPI",
      "configuration": {
           "type": "object",
           "properties": {
             "server.url": {
               "type": "string",
               "required": false,
               "default": "",
               "label": "Url",
               "description": ""
             },
             "server.key": {
               "type": "string",
               "required": false,
               "default": "",
               "label": "Key",
               "description": ""
             },
             "server.value": {
               "type": "string",
               "required": false,
               "default": "",
               "label": "Value",
               "description": ""
             }
           }
        }
     },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  },
  "dependencies": {
    "lodash": "*",
    "lodash-es": "^4.17.21"
  }
}
