{
  "name": "postcat-import-eolink",
  "version": "1.0.3",
  "title": "Import Eolink",
  "description": "Import Eolink",
  "main": "dist/index.js",
  "author": "Postcat",
  "logo": "https://www.eolink.com/assets/images/favicon.ico",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js"
  },
  "devDependencies": {
    "esbuild": "^0.14.38",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1"
  },
  "categories": ["Data Migration"],
  "features": {
    "importAPI": {
      "action": "importFunc",
      "label": "Eolink(.json)",
      "description": "${function.description}",
      "icon": "https://www.eolink.com/assets/images/favicon.ico"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
